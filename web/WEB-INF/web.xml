    <servlet>
        <servlet-name>feedBackServelet</servlet-name>
        <servlet-class>com.java.miniproject.servelet.feedBackServelet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChatFormServlet</servlet-name>
        <servlet-class>com.java.miniproject.servelet.ChatFormServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FeedBackReport</servlet-name>
        <servlet-class>com.java.miniproject.servelet.FeedBackReport</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OriginalFeedBack</servlet-name>
        <servlet-class>com.java.miniproject.servelet.OriginalFeedBack</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServeletLogin</servlet-name>
        <url-pattern>/ServeletLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FacultyServeletLogin</servlet-name>
        <url-pattern>/FacultyServeletLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MailSendingServelet</servlet-name>
        <url-pattern>/MailSendingServelet</url-pattern>
