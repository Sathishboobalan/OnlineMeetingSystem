        <servlet-name>logoutServelet</servlet-name>
        <url-pattern>/logoutServelet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>feedBackServelet</servlet-name>
        <url-pattern>/feedBackServelet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChatFormServlet</servlet-name>
        <url-pattern>/ChatFormServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FeedBackReport</servlet-name>
        <url-pattern>/FeedBackReport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OriginalFeedBack</servlet-name>
        <url-pattern>/OriginalFeedBack</url-pattern>
    </servlet-mapping>
